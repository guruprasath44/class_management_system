      </a>

      <!-- Sidebar -->
      <div class="sidebar">
        <!-- Sidebar user panel (optional) -->
        <div class="user-panel mt-3 pb-3 mb-3 d-flex">
          <div class="image">
            <img src="/img/default_profile.png" class="img-circle elevation-2" alt="User Image">
          </div>
          <div class="info">
            <a href="/student/student_profile" class="d-block">
              <%= userData%>
            </a>
          </div>
        </div>

        <!-- SidebarSearch Form -->
        <div class="form-inline">
          <div class="input-group" data-widget="sidebar-search">
            <input class="form-control form-control-sidebar" type="search" placeholder="Search" aria-label="Search">
            <div class="input-group-append">
              <button class="btn btn-sidebar">
                <i class="fas fa-search fa-fw"></i>
              </button>
            </div>
          </div>
        </div>

        <!-- Sidebar Menu -->
        <nav class="mt-2">
          <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
            <!-- Add icons to the links using the .nav-icon class
               with font-awesome or any other icon font library -->
            <li class="nav-item ">
              <a href="/student/dashboard" class="nav-link ">
                <i class="nav-icon fas fa-tachometer-alt"></i>
                <p>
                  Dashboard
                  <!-- <i class="right fas fa-angle-left"></i> -->
                </p>
              </a>

            <li class="nav-item menu-open">
              <a href="#" class="nav-link ">
                <i class="nav-icon fas fa-th"></i>
                <p>
                  Class
                  <i class="fas fa-angle-left right"></i>
                  <span class="right badge badge-danger"></span>
                </p>
              </a>
              <ul class="nav nav-treeview">
                <li class="nav-item menu-open">
                  <a href="#" class="nav-link">
                    <i class="far fa-circle nav-icon"></i>
                    <p>
                      III IT
                      <i class="right fas fa-angle-left"></i>
                    </p>
                  </a>
                  <ul class="nav nav-treeview">
                    <li class="nav-item">
                      <a href="/student/class_student" class="nav-link ">
                        <i class="far fa-dot-circle nav-icon"></i>
                        <p>Class Info</p>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a href="/student/subject_info" class="nav-link active ">
                        <i class="far fa-dot-circle nav-icon"></i>
                        <p>Subject Info</p>
                      </a>
                    </li>
                  </ul>
                </li>
              </ul>
            </li>


            <li class="nav-item">
              <a href="#" class="nav-link">
                <i class="nav-icon fas fa-edit"></i>
                <p>
                  Edit
                  <i class="fas fa-angle-left right"></i>
                </p>
              </a>
              <ul class="nav nav-treeview">

                <li class="nav-item">
                  <a href="/student/profile" class="nav-link ">
                    <i class="far fa-dot-circle nav-icon"></i>
                    <p> Edit My Profile</p>
                  </a>
                </li>

              </ul>
            </li>
          </ul>
        </nav>
        <!-- /.sidebar-menu -->
      </div>
      <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0">Subject Info</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item">Dashboard</a></li>
                <li class="breadcrumb-item ">Class </li>
                <li class="breadcrumb-item active">Subject Info </li>
              </ol>
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->


      <!-- Main content -->
      <section class="content">
        <h3 class="mt-4 mb-4">Subject Info</h3>

        <div class="row ">
          <div class="col-md-4">
            <!-- Widget: user widget style 2 -->
            <div class="card card-widget widget-user-2">
              <!-- Add the bg color to the header using any of the bg-* classes -->
              <div class="widget-user-header bg-warning">
                <div class="widget-user-image">
                  <img class="img-circle elevation-2" src="/img/it_profile.png" id="it_prof" alt="User Avatar">
                </div>

                <h3 class="widget-user-username">III Year </h3>
                <h5 class="widget-user-desc">Information Technology</h5>
              </div>

            </div>

          </div>
        </div>

        <div class="row">
          <form action="/student/semester" method="post">
            <div class=" col-12 form-group row">

              <label for="inputEmail" class="col-sm-4 col-form-label">Semester</label>
              <div class="col-sm-6">
                <select class="form-control select2" id="semester" name="semester" style="width: 100%;">
                  <option value="">Choose</option>
                  <option value="5">Semester 5</option>
                  <option value="6">Semester 6</option>


                </select>
              </div>

              <div class="col-md-2">
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </div>
          </form>
        </div>

        <!-- /.card-header -->

        <div class="row subjects-list">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title ">Semester <%= sem%>
                </h3>
              </div>
            </div>
          </div>
        </div>
        <!-- /.card-header -->
        <div class="card-body table-responsive p-0" style="height: 500px;">
          <table class="table table-hover text-nowrap ">
            <thead>

              <tr>
                <th>S.No</th>
                <th>Subject Code</th>
                <th>Subject </th>
                <th>Faculty</th>
                <th>Credit</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <% if(subjectData.length!=0){%>
                  <%var i=1;%>
                    <%subjectData.forEach(function(data){%>
                      <td>
                        <%=i;%>
                      </td>
                      <td>
                        <%=data.sub_code %>
                      </td>
                      <td>
                        <%=data.sub_name%>
                      </td>
                      <td>
                        <%=data.faculty_name %>
                          <%=data.pos%>
                      </td>
                      <td>
                        <%=data.credit %>
                      </td>
              </tr>
              <% i++ })%>
                <% } %>


            </tbody>
          </table>
        </div>
        <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </div>
  </div>

  </section>

  <!-- /.Left col -->
  <!-- right col (We are only adding the ID to make the widgets sortable)-->
  <section class="col-lg-5 connectedSortable">
    </div>

    <div class="card-body pt-0">
      <!--The calendar -->
      <div id="calendar" style="width: 100%"></div>
    </div> -->
    <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </section>
  <!-- right col -->
  </div>
  <!-- /.row (main row) -->
  </div><!-- /.container-fluid -->
  </section>
  <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <!-- Control Sidebar -->
  <!-- Control sidebar content goes here -->
  <aside class="control-sidebar control-sidebar-dark">

  </aside>
  <!-- /.control-sidebar -->
  </div>
