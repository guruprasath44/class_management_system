          </div>
          <!-- /.card-header -->
          <div class="card-body col-12">
            <span class="tootltip-r" data-toggle="tooltip" data-placement="left"
              title="Select 6 columns from column visibility to view in pdf"></span>
            <table width="50" id="example1" class="table table-bordered table-responsive students-list">

              <thead>
                <tr class="col-12">
                  <th>S.No</th>
                  <th>FirstName</th>
                  <th>LastName</th>
                  <th>Roll No</th>
                  <th>Reg. No</th>
                  <th>Email</th>
                  <th>Academic year</th>
                  <th>DOB</th>
                  <th>Hostler/Dayscholar</th>
                  <th>Gender</th>
                  <th>Mob.No</th>
                  <th>Address</th>
                  <th>City</th>
                  <th>State</th>
                  <th>Pincode</th>

                </tr>
              </thead>
              <tbody>
                <% if(studentData.length!=0){%>
                  <%var i=1;%>
                    <%studentData.forEach(function(data){%>
                      <tr>
                        <td>
                          <%=i;%>
                        </td>
                        <td>
                          <%=data.firstname %>
                        </td>
                        <td>
                          <%=data.lastname %>
                        </td>
                        <td>
                          <%=data.rollno %>
                        </td>
                        <td>
                          <%=data.regno %>
                        </td>
                        <td>
                          <%=data.email %>
                        </td>
                        <td>
                          <%=data.Batch%>
                        </td>
                        <% var datemonth=data.dob.getMonth()+1 %>
                          <td>
                            <%=data.dob.getDate()+" /"+datemonth +"/"+ data.dob.getFullYear()%>
                          </td>
                          <td>
                            <%=data.hos_day %>
                          </td>
                          <td>
                            <%=data.gender %>
                          </td>
                          <td>
                            <%=data.mobileno %>
                          </td>
                          <td>
                            <%=data.address %>
                          </td>


                          <td>
                            <%=data.city%>
                          </td>
                          <td>
                            <%=data.state%>
                          </td>
                          <td>
                            <%=data.pincode%>
                          </td>
                      </tr>
                      <% i++ })%>
                        <% }%>



              </tbody>
              <!-- <tfoot>
